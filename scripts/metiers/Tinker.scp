[ITEMDEF m_prod_piece]
NAME=prod piece
ID=i_memory
TYPE=t_eq_script

ON=@CREATE
TIMER={2 3}

ON=@TIMER
IF <CONT.FINDID.i_poussiere_d_or>
    VAR.FACTEUR = <EVAL (<CONT.BLACKSMITHING>/15) + 20> //30.0 => 40 ; 150.0 => 120
    IF <CONT.FINDID.i_poussiere_d_or.AMOUNT> > 20
        CONT.FINDID.i_poussiere_d_or.AMOUNT = <EVAL <CONT.FINDID.i_poussiere_d_or.AMOUNT> - 20>
        CONT.NEWITEM i_gold
        CONT.ACT.AMOUNT = <EVAL 10 * <VAR.FACTEUR>>
        CONT.ACT.CONT = <CONT.UID>
    ELSE
        CONT.NEWITEM i_gold
        CONT.ACT.AMOUNT = <EVAL (<CONT.FINDID.i_poussiere_d_or.AMOUNT> * <VAR.FACTEUR>)/2>
        CONT.ACT.CONT = <CONT.UID>
        CONT.FINDID.i_poussiere_d_or.REMOVE
    ENDIF
    VAR.FACTEUR
ENDIF
REMOVE
RETURN 1
