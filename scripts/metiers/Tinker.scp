[ITEMDEF i_pioche_de_carriere]
DEFNAME=i_pioche_de_carriere
NAME=Pioche de carriere
ID=i_pickaxe
RESOURCES=1 i_log, 1 i_ingot_iron
SKILLMAKE=TINKERING 40.0, t_tinker_tools

ON=@Create
HITPOINTS={100 200}
COLOR=0386

ON=@DClick
TARGET Ou voulez vous piocher ?
RETURN 1

ON=@TARGON_ITEM
IF (<SRC.TARG.TYPE>==t_arock
    IF <SRC.FINDID.m_limitateur_carriere>
        SRC.SYSMESSAGE Vous retroussez vos manches , essuyez la sueur de votre front et etes pret a repiocher
    ELSE
        SRC.NEWITEM=m_limitateur_carriere
        SRC.ACT.EQUIP
        IF (50.0 < <SRC.MINING>)&&(<SRC.MINING> < 60.0)
            IF (<EVAL(RAND(25))>=0) 
                SRC.GAGNERPIERRE 
            ELSE 
                SRC.PASDEPIERRE 
            ENDIF
        ELSEIF (60.0 < <SRC.MINING>)&&(<SRC.MINING> < 70.0)
            IF (<EVAL(RAND(20))>=0) 
                SRC.GAGNERPIERRE 
            ELSE 
                SRC.PASDEPIERRE 
            ENDIF
        ELSEIF (70.0 < <SRC.MINING>)&&(<SRC.MINING> < 80.0)
            IF (<EVAL(RAND(15))>=0) 
                SRC.GAGNERPIERRE 
            ELSE 
                SRC.PASDEPIERRE 
            ENDIF
        ELSEIF (80.0 < <SRC.MINING>)&&(<SRC.MINING> < 100.0)
            IF (<EVAL(RAND(10))>=0) 
                SRC.GAGNERPIERRE 
            ELSE 
                SRC.PASDEPIERRE 
            ENDIF
        ELSEIF <SRC.MINING> > 99.9
            IF (<EVAL(RAND(5))>=0) 
                SRC.GAGNERPIERRE 
            ELSE 
                SRC.PASDEPIERRE 
            ENDIF
        ELSE
            SRC.PASDEPIERRE
        ENDIF
    ENDIF
ELSE
    IF <SRC.TARG.TYPE>==t_rock
        RETURN 0
    ELSE
        SRC.SYSMESSAGE Vous ne pouvez pas extraire de pierre de <SRC.TARG.NAME> !
    ENDIF
ENDIF

CATEGORY=Genese
SUBSECTION=Tailleur
DESCRIPTION=Pioche de carriere

[ITEMDEF m_limitateur_carriere]
ID=i_memory
TYPE=t_eq_script

ON=@CREATE
TIMER=2

ON=@TIMER
REMOVE
RETURN 1

[Function GAGNERPIERRE]
IF <SRC.STAMINA>>10
SRC.TAG.TempStam=<SRC.STAMINA>-10 
SRC.STAMINA=<SRC.TAG.TempStam> 
SRC.ANIM 11
IF (<EVAL(RAND(10))>=0) 
SRC.NEWITEM i_pierre_solide
SRC.ACT.BOUNCE
SRC.SYSMESSAGE Malgre la fatigue vous reussissez a extraire une pierre de cette roche
ELSE 
SRC.NEWITEM i_poussiere_de_roche
SRC.ACT.BOUNCE
SRC.SYSMESSAGE Vous n'arrivez a extraire que de la poussiere de cette roche
ENDIF
ELSE
SRC.SYSMESSAGE Vous etes trop fatigue pour continuer de piocher
ENDIF

[Function PASDEPIERRE]
IF <SRC.STAMINA>>10
SRC.TAG.TempStam=<SRC.STAMINA>-10
SRC.STAMINA=<SRC.TAG.TempStam> 
SRC.ANIM 11
SRC.SYSMESSAGE Malgre vos efforts soutenus, vous n'arrivez pas a extraire de la pierre de cette roche
ELSE
SRC.SYSMESSAGE Vous etes trop fatigue pour continuer de piocher
ENDIF

