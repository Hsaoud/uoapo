[ITEMDEF i_poussiere_d_or]
NAME=Poussiere d'or
ID=0eef
TYPE=t_normal
VALUE=1

ON=@Create
COLOR=color_o_gold

CATEGORY=Genese
SUBSECTION=Tinker
DESCRIPTION=Poussiere d'or

//AU TIMER, DONNE LA POUSSIERE
[ITEMDEF m_prod_poussiere_or]
NAME=prod pouss or
ID=i_memory
TYPE=t_eq_script

ON=@CREATE
TIMER={2 3}

ON=@TIMER
IF <CONT.FINDID.i_ingot_gold>
    VAR.FACTEUR = <EVAL (<CONT.TINKERING>/15) + 20> //30.0 => 40 ; 150.0 => 120
    IF <CONT.FINDID.i_ingot_gold.AMOUNT> > 10
        CONT.FINDID.i_ingot_gold.AMOUNT = <EVAL <CONT.FINDID.i_ingot_gold.AMOUNT> - 10>
        CONT.NEWITEM i_poussiere_d_or
        CONT.ACT.AMOUNT = <EVAL <VAR.FACTEUR>>
        CONT.ACT.CONT = <CONT.UID>
    ELSE
        CONT.NEWITEM i_poussiere_d_or
        CONT.ACT.AMOUNT = <EVAL (<CONT.FINDID.i_ingot_gold.AMOUNT> * <VAR.FACTEUR>)/10>
        CONT.ACT.CONT = <CONT.UID>
        CONT.FINDID.i_ingot_gold.REMOVE
    ENDIF
    VAR.FACTEUR
ENDIF
REMOVE
RETURN 1

