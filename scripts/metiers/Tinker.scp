[ITEMDEF i_serrure_pour_container]
DEFNAME=i_serrure_pour_container
NAME=Serrure pour container
ID=i_oil_flask
TYPE=T_SCRIPT
RESOURCES=1 i_ingot_iron
SKILLMAKE=TINKERING 100.0, t_tinker_tools

ON=@DCLICK
IF <MORE1>==0
    SRC.SYSMESSAGEUA 0481 1 1 0 Veuillez selectionner le container sur lequel vous desirez inserer une serrure
    MORE1=1
    TARGET
ELSE
    SRC.SYSMESSAGEUA 0481 1 1 0 Veuillez selectionner la clef que vous desirez faire correspondre au container precedemment choisi
    MORE1=2
    TARGET
ENDIF
RETURN 1

ON=@TARGON_ITEM
IF (<SRC.TARG.TOPOBJ.UID>==<SRC.UID>)
    IF !(<SRC.TARG.UID>==<UID>)
        IF <MORE1>==1
            IF !(<SRC.TARG.BASEID>==i_backpack)
                IF <SRC.TARG.TYPE>==T_CONTAINER
                    IF <SRC.TARG.MORE1>==0
                        SRC.TARG.MORE1=<SRC.TARG.UID>
                        TAG.CONTAINERUID=<SRC.TARG.UID>
                        TAG.CONTAINERNOM=<SRC.TARG.NAME>
                        MORE1=2
                        USE
                    ELSE
                        SRC.SYSMESSAGEUA 0481 1 1 0 <SRC.TARG.NAME> possede deja une serrure !
                        MORE1=0
                    ENDIF
                ELSEIF (<SRC.TARG.BASEID> == i_portiere_en_frene_est) || (<SRC.TARG.BASEID> == i_portiere_en_frene_ouest) || (<SRC.TARG.BASEID> == i_portiere_en_frene_nord) || (<SRC.TARG.BASEID> == i_portiere_en_frene_sud)
                    IF <SRC.TARG.MORE1>==0
                        SRC.TARG.MORE1=<SRC.TARG.UID>
                        TAG.CONTAINERUID=<SRC.TARG.UID>
                        TAG.CONTAINERNOM=<SRC.TARG.NAME>
                        MORE1=2
                        USE
                    ELSE
                        SRC.SYSMESSAGEUA 0481 1 1 0 <SRC.TARG.NAME> possede deja une serrure !
                        MORE1=0
                    ENDIF
                ELSE
                    SRC.SYSMESSAGEUA 0481 1 1 0 <SRC.TARG.NAME> n'est pas un container !
                    MORE1=0
                ENDIF
            ELSE
                SRC.SYSMESSAGEUA 0481 1 1 0 Vous ne pouvez pas inserer une serrure sur un sac a dos !
                MORE1=0
            ENDIF
        ELSE
            IF <SRC.TARG.TYPE>==T_KEY
                IF <SRC.TARG.MORE1>==0
                    SRC.TARG.MORE1=<TAG.CONTAINERUID>
                    SRC.TARG.NAME Cle pour <TAG.CONTAINERNOM>
                    //REMOVE
                    IF <SRC.FINDID.i_serrure_pour_container.amount> >1
                        SRC.FINDID.i_serrure_pour_container.amount=<EVAL <SRC.FINDID.i_serrure_pour_container.amount>-1>
                        FIX
                    ELSE
                        REMOVE
                    ENDIF
                ELSE
                    SRC.SYSMESSAGEUA 0481 1 1 0 <SRC.TARG.NAME> n'est pas vierge !
                    MORE1=0
                    TRY UID.<TAG.CONTAINERUID>.MORE1=0
                ENDIF
            ELSE
                SRC.SYSMESSAGEUA 0481 1 1 0 <SRC.TARG.NAME> n'est pas une clef !
                TRY UID.<TAG.CONTAINERUID>.MORE1=0
                MORE1=0
            ENDIF
        ENDIF
    ELSE
        SRC.SYSMESSAGEUA 0481 1 1 0 Reinitialisation de la serrure
        MORE1=0
    ENDIF
ELSE
    SRC.SYSMESSAGEUA 0481 1 1 0 Vous devez placer <SRC.TARG.NAME> dans votre sac pour realiser cette operation !
    MORE1=0
ENDIF
RETURN 1

CATEGORY=Genese
SUBSECTION=Tinker
DESCRIPTION=Serrure pour container

