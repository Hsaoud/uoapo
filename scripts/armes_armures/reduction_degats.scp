//DEFAULT = JOUEUR OU MONSTRE FRAPPE
[FUNCTION GET_REDUCTION_DEGATS]
VAR.TOUCHE_OU = <EVAL RAND(100)>//[0...99]
//VENTRE
IF <VAR.TOUCHE_OU> < 35
	VAR.TOUCHE_OU
	IF <FINDLAYER.5>
		IF <EVAL 0<FINDLAYER.5.TAG.DOMMAGESREDUITS>> > 0
			RETURN <FINDLAYER.5.TAG.DOMMAGESREDUITS>
		ENDIF
	ELSEIF <SRC.FINDLAYER.13>
		IF <EVAL 0<FINDLAYER.13.TAG.DOMMAGESREDUITS>> > 0
			RETURN <FINDLAYER.13.TAG.DOMMAGESREDUITS>
		ENDIF
	ENDIF
//JAMBES
ELSEIF <VAR.TOUCHE_OU> < 55
	VAR.TOUCHE_OU
	IF <FINDLAYER.4>
		IF <EVAL 0<FINDLAYER.4.TAG.DOMMAGESREDUITS>> > 0
			RETURN <FINDLAYER.4.TAG.DOMMAGESREDUITS>
